<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="90"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="Copy PICs to Pictures Dir   "
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_copy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />


            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="Load PICs From Pic Dir    "
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_copy2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="Rebuild Master File            "
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_rebuild_master"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="Manage internal Activities "
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_manage_internal_activities"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="Manage external Activities "
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_manage_external_activities"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="Copy PICs to Gallery           "
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_copyPICstoGallery"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="Validation Check for PICs   "
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_validationcheck4pics"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="Add Unregistered Pictures"
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_Add_Unregistered_Pictures"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView123"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="check Currupted Image Files"
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_checkCurrptedPic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="80dp"
                    android:text="Activity Rebuild Daily"
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton_rebuildDailyActivities"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@android:color/transparent"
                    android:onClick="onClick"
                    app:srcCompat="@drawable/oksign_32" />
            </TableRow>

        </TableLayout>


    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="10">

        <Button
            android:id="@+id/bt_admin_back"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="100dp"
            android:layout_marginRight="80dp"
            android:layout_marginBottom="10dp"
            android:layout_weight="33"
            android:onClick="onClick"
            android:text="Ok" />

    </LinearLayout>
</LinearLayout>
